<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title">New Competency</h3>
  </div>
  <!-- /.box-header -->
  <!-- form start -->
  <%= form_for @user_competency_level, url: @url, role: :form, html: { multipart: true } do |f| %>

    <div class="box-body">
      <div class="form-group">
        <%= f.label :user_id %>
        <%= f.collection_select(:user_id, @users, :id, :name, {}, { class: "form-control select2 select2-hidden-accessible", style: "width: 100%" } ) %>
      </div>
      <div class="form-group">
        <%= f.label :competency_id %>
        <%= f.collection_select(:competency_id, @competencies, :id, :name, {}, { class: "form-control select2 select2-hidden-accessible", style: "width: 100%" } ) %>
      </div>
      <div class="form-group">
        <%= f.label :level_id %>
        <%= f.collection_select(:level_id, @levels, :id, :name, {}, { class: "form-control select2 select2-hidden-accessible", style: "width: 100%" } ) %>
      </div>
      <div class="form-group">
        <%= f.label :started_date %>
        <%= f.text_field(:started_date, {class: 'form-control', id: 'datetimepicker'}) %>
      </div>
    </div>

    <!-- /.box-body -->

    <div class="box-footer">
      <%= f.submit class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
